<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
<cron>
    <url>/admin/crons/DriverLocationCheckServlet</url>
    <description>check drivers' states for possible errors</description>

    <schedule>every 4 hours</schedule>
    <retry-parameters>
        <min-backoff-seconds>2.5</min-backoff-seconds>
        <max-doublings>5</max-doublings>
    </retry-parameters>
</cron>
</cronentries>